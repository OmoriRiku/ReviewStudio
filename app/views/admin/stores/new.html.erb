<!--店舗の新規作成-->
<div class="container">
  <h2 class="text-center">店舗新規作成</h2>>
  <div class="row">
    <div class="col-md-9 mx-auto">
      <%= render 'layouts/errors', obj: @store %>
      <div class="mt-5">
      <%= form_with model: @store, url: stores_path, method: :post do |f| %>
        <%= render :partial => 'form', :locals => { :f => f } %>
      <% end %>
      </div>
    </div>
  </div>
</div>