<!--店舗レビュー作成-->
<div class="container">
  <h3 class="text-center"><%= @store.name %>のレビュー投稿</h3>
  <div class="row">
    <div class="col-md-9 mx-auto">
      <div class="mt-5">
      <%= render 'layouts/errors', obj: @store_review %>
      <%= form_with model: [@store, @store_review], url: store_store_reviews_path, method: :post do |f|%>
        <%= render :partial => 'form', :locals => { :f => f } %>
      <% end %>
      </div>
    </div>
  </div>
</div>